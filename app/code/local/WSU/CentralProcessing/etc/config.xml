<?xml version="1.0"?>
<!--
/**
 * @category   Cybersource
 * @package    Wsu_CentralProcessing
 */
 -->
<config>
    <modules>
        <Wsu_CentralProcessing>
            <version>0.0.1</version>
        </Wsu_CentralProcessing>
    </modules>
    <global>
        <models>
            <centralprocessing>
                <class>Wsu_CentralProcessing_Model</class>
            </centralprocessing>
        </models>
        <resources>
			<centralprocessing_setup>
				<setup>
					<module>Wsu_CentralProcessing</module>
				</setup>
				<connection>
					<use>core_setup</use>
				</connection>
			</centralprocessing_setup>
			<centralprocessing_write>
				<connection>
					<use>core_write</use>
				</connection>
			</centralprocessing_write>
			<centralprocessing_read>
				<connection>
					<use>core_read</use>
				</connection>
			</centralprocessing_read>
        </resources>
        <blocks>
            <centralprocessing>
                <class>Wsu_CentralProcessing_Block</class>
            </centralprocessing>
        </blocks>
        <helpers>
            <centralprocessing>
                <class>Wsu_CentralProcessing_Helper</class>
            </centralprocessing>
        </helpers>
    </global>
    <frontend>
        <routers>
            <centralprocessing>
                <use>standard</use>
                <args>
                    <module>Wsu_CentralProcessing</module>
                    <frontName>centralprocessing</frontName>
                </args>
            </centralprocessing>
        </routers>
        <layout>
            <updates>
                <centralprocessing>
                    <file>centralprocessing.xml</file>
                </centralprocessing>
            </updates>
        </layout>
    </frontend>
    <adminhtml>
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <cactimediainfo>
											<title>Cybersource - All</title>
										</cactimediainfo>
										<paymentinfo>
											<title>CyberSource Secure Acceptance WM Info</title>
										</paymentinfo>
										<centralprocessing>
											<title>CyberSource Secure Acceptance WM Area</title>
										</centralprocessing>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
    <default>
        <payment>
            <centralprocessing>
                <active>0</active>
				<enable_log>1</enable_log>
               	<model>centralprocessing/centralprocessing</model>
               	<title>Credit Card</title>
                <mode>0</mode>
				<order_status>pending</order_status>
                <debug_flag>1</debug_flag>
				<allowspecific>0</allowspecific>
				<sort_order>1</sort_order>
            </centralprocessing>
        </payment>
    </default>
</config>